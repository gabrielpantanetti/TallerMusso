{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "TallerMusso": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/TallerMusso",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [              
              "src/assets/css/magnific-popup.css"
              ,"src/assets/css/animate.min.css"
              ,"src/assets/css/font-awesome.min.css"            
              ,"src/assets/css/nivo-lightbox.css"            
              ,"src/assets/css/nivo_themes/default/default.css"            
              ,"src/assets/css/hover-min.css"            
              ,"src/assets/css/flexslider.css"            
              ,"src/assets/css/style.css"            
              ,"src/assets/css/icofont.css"            
              ,"src/styles.css"
            ],
            "scripts": ["src/assets/js/jquery.js", "src/assets/js/bootstrap.min.js"
              , "src/assets/js/jquery.magnific-popup.min.js", "src/assets/js/jquery.sticky.js"
              , "src/assets/js/jquery.backstretch.min.js", "src/assets/js/isotope.js"
              , "src/assets/js/imagesloaded.min.js", "src/assets/js/nivo-lightbox.min.js"
              , "src/assets/js/jquery.flexslider-min.js", "src/assets/js/jquery.parallax.js"
              , "src/assets/js/smoothscroll.js"
              , "src/assets/js/wow.min.js"
              , "src/assets/js/iconify.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "TallerMusso:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "TallerMusso:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "TallerMusso:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "TallerMusso:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "TallerMusso:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "TallerMusso",
  "cli": {
    "analytics": "4f63fdf2-a913-4c9b-bfdc-1db75df10799"
  }
}
